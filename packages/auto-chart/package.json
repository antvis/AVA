  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.4",
    "@types/antd": "^1.0.0",
    "@types/lodash": "^4.14.175",
    "@types/react": "^17.0.27",
    "@types/react-dom": "^17.0.9",
