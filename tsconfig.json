{
  "compilerOptions": {
    "target": "ES5",
    "module": "commonjs",
    "lib": ["dom", "es2017"],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "declaration": true,
    "importHelpers": true,
    "downlevelIteration": true,
    "strict": true,
    "outDir": "./lib",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "moduleResolution": "node"
  }
}
